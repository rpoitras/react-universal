  "main": "app/index.js",
  "name": "react-universal",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "clean:docker": "docker container rm react-universal ; docker image rm react-universal-prod-i",
    "build:docker": "docker build -t react-universal-prod-i .",
    "run:docker": "docker run --name react-universal -p 443:443 react-universal-prod-i",
    "stop:docker": "docker stop react-universal",
    "docker": "yarn run stop:docker && yarn run clean:docker && yarn run build:docker && yarn run run:docker",
    "build": "yarn run clean && yarn run lint:fix && NODE_ENV=production webpack -p",
    "build:dev": "yarn run lint:fix && NODE_ENV=development webpack --progress",
    "lint": "eslint --ext=jsx --ext=js ./src",
    "lint:fix": "eslint --ext=jsx --ext=js --fix ./app",
    "start": "yarn run yarn:install && NODE_ENV=development node server/main",
    "start:nodemon": "yarn run yarn:install && NODE_ENV=development nodemon server/main",
    "start:prod": "yarn run test && NODE_ENV=production node server/main",
    "start:dev": "NODE_ENV=development node server/main",
    "start:wss": "NODE_ENV=development node server/websocket-server",
    "test": "NODE_PATH=./app mocha --opts mocha.opts",
    "test:watch": "NODE_PATH=./app mocha --watch --opts ./mocha.opts",
    "yarn:install": "yarn install"
  },
  "version": "1.0.0"
